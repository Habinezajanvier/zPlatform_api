{
  "name": "z-platform-server",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && npx tsc",
    "start": "dotenv -e .env.dev -- node dist/index.js",
    "dev": "dotenv -e .env.dev -- concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "dotenv -e .env.test jest --coverage",
    "test:watch": "jest --watch",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev --name dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name test",
    "migrate:undo:test": "dotenv -e .env.test -- npx prisma migrate reset",
    "migrate:undo:dev": "dotenv -e .env.dev -- npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nodemailer": "^6.4.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.4.0",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
  }
}
